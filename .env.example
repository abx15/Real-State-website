# Real Estate Application Configuration
# Copy this file to .env.local and update with your values

# Application Settings
VITE_APP_NAME=Estatery
VITE_APP_DESCRIPTION=Luxury Real Estate Platform

# API Configuration
# VITE_API_URL=https://api.example.com
# VITE_API_KEY=your_api_key_here

# Firebase Configuration (if using Firebase backend)
# VITE_FIREBASE_API_KEY=your_firebase_api_key
# VITE_FIREBASE_PROJECT_ID=your_firebase_project_id
# VITE_FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket

# Email Configuration
# VITE_EMAIL_SERVICE_ID=your_email_service_id
# VITE_EMAIL_TEMPLATE_ID=your_email_template_id
# VITE_EMAIL_PUBLIC_KEY=your_email_public_key

# Google API (for Maps, Analytics, etc.)
# VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
# VITE_GOOGLE_ANALYTICS_ID=your_google_analytics_id

# Authentication
# VITE_AUTH_REDIRECT_URL=http://localhost:5173/callback
# VITE_JWT_SECRET=your_jwt_secret

# Feature Flags
# VITE_ENABLE_WISHLIST=true
# VITE_ENABLE_MESSAGING=true
# VITE_ENABLE_VIRTUAL_TOURS=false

# Development Settings
VITE_LOG_LEVEL=info
VITE_DEV_MODE=false
